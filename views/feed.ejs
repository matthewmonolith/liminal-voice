<%- include('header') -%>
<body class="posts-body">
  <div class="container">
    <%- include('sidebar') -%>
    <main class="posts">
      <h1>Posts Made By Users Exploring Liminal Spaces</h1>
      <div class="posts-feed">
        <ul class="posts-list">
          <% for(var i=0; i<posts.length; i++) {%>
            <li class="post">
              <a href="/post/<%= posts[i]._id%>">
                <img src="<%= posts[i].image%>">
              </a>
            </li>
          <% } %>
        </ul>
      </div>
    </main>
  </div>
</body>
